<SelectButton@Button>:
    Image:
        source: 'buttonInProgress.png'
        allow_strech: True
        keep_ratio: False
        pos: self.parent.pos
        size: self.parent.size
        
<LayoutScreen@BoxLayout>:
    orientation: 'horizontal'

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'retrobackground2.jpg'
    
    BoxLayout:
        orientation: 'vertical'
        Label:
            id: img1
            text: 'Image1'
        Label:
            id: img2
            text: 'Image2'
        Label:
            id: img3
            text: 'Image3'

    BoxLayout:
        orientation: 'vertical'
        Label:
            text: ''
        SelectButton:
            id: selectbtn
            pos_hint:{'center_x':0.5, 'center_y':0.5}
            size_hint: 0.45, 0.11
        Label:
            text: ''
            
        
    BoxLayout:
        orientation: 'vertical'
        Label:
            id: img4
            text: 'Image4'
        Label:
            id: img5
            text: 'Image5'
        Label:
            id: img6
            text: 'Image6'